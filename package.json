{
  "name": "@coinbase/coinbase-cloud-sdk",
  "version": "0.0.1",
  "description": "Coinbase Cloud JavaScript SDK",
  "keywords": [
    "coinbase",
    "coinbasecloud",
    "eth",
    "ethereum",
    "node",
    "web3"
  ],
  "author": "Coinbase Cloud",
  "license": "Apache-2.0",
  "scripts": {
    "prepublishOnly": "yarn build && yarn run lint && yarn run test",
    "prebuild": "yarn clean && tsc --emitDeclarationOnly --declaration -p tsconfig.json",
    "build": "rollup -c",
    "clean": "rm -rf dist",
    "format": "prettier --write '**/*.{json,ts}'",
    "lint": "eslint -c .eslintrc.js '**/*.ts' --ignore-path '.gitignore'",
    "test": "jest test/unit",
    "test-integration": "jest test/integration",
    "docs": "typedoc --plugin typedoc-plugin-markdown --out docs-md src/index.ts"
  },
  "main": "dist/cjs/index.js",
  "main-es": "dist/es/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    "node": {
      "types": "./dist/src/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/cjs/index.js"
    },
    "package.json": "./package.json",
    "default": "./dist/esm/index.js"
  },
  "types": "./dist/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coinbase/coinbase-cloud-sdk-js.git"
  },
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/coinbase/coinbase-cloud-sdk-js/issues"
  },
  "homepage": "https://github.com/coinbase/coinbase-cloud-sdk-js#readme",
  "dependencies": {
    "@ethersproject/abstract-provider": "^5.7.0",
    "@ethersproject/networks": "^5.7.1",
    "@ethersproject/providers": "^5.7.2",
    "@ethersproject/web": "^5.7.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^23.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "@types/jest": "^29.2.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.26.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.2.1",
    "prettier": "2.7.1",
    "prettier-plugin-jsdoc": "^0.4.2",
    "rollup": "^2.79.1",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-jest": "^29.0.3",
    "tslib": "^2.4.0",
    "typedoc": "^0.23.17",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "^4.8.4"
  },
  "sideEffects": false
}
